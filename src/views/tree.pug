doctype html
html
  head
    title="Tree"
  body
    ul(class="tree-list")
      each object in objects
        li(class="tree-list__item")
          -if (object.type === 'link-back')
            div(class="link-back")
              a(class="link-back__link", href='/tree?commit=' + object.commit + '&path=' + object.path)=".."
          -if (object.type === 'blob')
            div(class="blob-object")
              a(class="blob-object__link", href='/blob?commit=' + object.commit + '&path=' + object.path)=object.path
          -if (object.type === 'tree')
            div(class="tree-object")
              a(class="tree-object__link", href='/tree?commit=' + object.commit + '&path=' + object.path)=object.path